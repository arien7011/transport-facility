{
  "name": "transport-facility",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss}\"",
    "lint:fix": "ng lint --fix",
    "analyze": "ng build --stats-json && npx webpack-bundle-analyzer dist/*/stats.json",
    "build:prod": "ng build --configuration production",
    "lighthouse": "lhci autorun"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.0",
    "@lhci/cli": "^0.15.1",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "^2.0.3",
    "angular-eslint": "20.2.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.40.0",
    "web-vitals": "^5.1.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "src/**/*.{ts,html,scss}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "script": {
    "format:check": "prettier --check \"src/**/*.{ts,html,scss}\"",
    "lint:fix": "ng lint --fix",
    "build:analyze": "ng build --stats-json",
    "analyze": "npx webpack-bundle-analyzer dist/transport-facility/stats.json",
    "build:prod": "ng build --configuration production"
  }
}
